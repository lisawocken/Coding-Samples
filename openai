# This coding sample is generated by an AI language model
# to demonstrate my interest in coding and my willingness to learn

import tensorflow as tf
import numpy as np

# Define a function to generate a random matrix
def generate_random_matrix(n_rows, n_cols):
    matrix = np.random.rand(n_rows, n_cols)
    return matrix

# Define a function to perform matrix multiplication
def matrix_multiplication(matrix1, matrix2):
    result = tf.matmul(matrix1, matrix2)
    return result

# Define the main function
def main():
    # Generate two random matrices
    matrix1 = generate_random_matrix(3, 4)
    matrix2 = generate_random_matrix(4, 2)

    # Perform matrix multiplication
    result = matrix_multiplication(matrix1, matrix2)

    # Print the result
    print(result)

if __name__ == '__main__':
    # Call the main function
    main()
